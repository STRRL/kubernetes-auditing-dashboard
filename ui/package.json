{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n next-dev,codegen,backend -c blue,green,cyan \"npm run next-dev\" \"npm run codegen-watch\" \"npm run backend\"",
    "next-dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen-watch": "graphql-codegen --config codegen.ts --watch",
    "backend": "cd ../ && CGO_ENABLED=1 go run ./cmd/kubernetes-auditing-dashboard"
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "@tanstack/react-query": "^4.29.1",
    "@types/node": "18.15.11",
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "10.4.14",
    "daisyui": "^2.51.5",
    "eslint": "8.37.0",
    "eslint-config-next": "13.3.0",
    "graphql": "^16.6.0",
    "graphql-request": "^6.0.0",
    "moment": "^2.29.4",
    "next": "13.3.0",
    "postcss": "8.4.21",
    "react": "18.2.0",
    "react-diff-viewer-continued": "^3.4.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.3.0",
    "@graphql-codegen/client-preset": "3.0.0",
    "@graphql-codegen/introspection": "3.0.1",
    "concurrently": "^8.0.1",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.3"
  }
}
